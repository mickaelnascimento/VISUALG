Algoritmo "EXERCICIO DE CONSUMO"




Var

   Distancia,Gasto,Consumo : real

Inicio

      Escreva("Digite a distancia percorrida: ")
      Leia(Distancia)
      Escreval
      Escreva("Digite o combustivel gasto: ")
      Leia(Gasto)
      escreval
      Consumo <- Distancia/Gasto
      Escreval("O Consumo medio do veiculo é ",Consumo:8:3)

Fimalgoritmo