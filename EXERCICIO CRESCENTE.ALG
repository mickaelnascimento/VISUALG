Algoritmo "EXERCICIO CRESCENTE"
// 




Var


  x,y : inteiro

Inicio

  x <- 0
  y <- 0
    escreval("Digite dois numeos: ")
  leia(x)
  leia(y)

 enquanto x<>y faca
  
  se x>y entao
     Escreval("Decrescente")
  senao
   Escreval("Crescente")
   fimse

  escreval("Digite outros dois numeos: ")
  leia(x)
  leia(y)

   fimenquanto


Fimalgoritmo
